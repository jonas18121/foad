<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>

        <header>
            <nav>
                <ul>
                    <li><a href='{{ path('ticket_all') }}'>Accueil</a></li>
                    <li><a href='{{ path('ticket_all') }}' >Ticket</a></li>
                    <li><a href='{{ path('ticket_add') }}' >Ajouter un ticket</a></li>
                </ul>
            </nav>
        </header>

        {% block body %}{% endblock %}
    </body>
</html>
